<!DOCTYPE html>
<html>
  <head>
    <title>Check</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <% if current_user%>
  <%= link_to "log out", session_path(current_user), method: :delete, :class=> "btn-btn"%>
  <%end%>
    <%= yield %>
  </body>
</html>
